{
  "name": "markdown-denden-furigana",
  "displayName": "Markdown DenDen Furigana",
  "description": "Adds denden markdown furigana support to VSCode's markdown preview",
  "version": "0.0.3",
  "license": "MIT",
  "publisher": "saeris",
  "repository": {
    "type": "git",
    "url": "https://github.com/saeris/markdown-denden-furigana.git"
  },
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "browser": "./dist/extension.js",
  "contributes": {
    "markdown.markdownItPlugins": true
  },
  "scripts": {
    "build": "vsce package",
    "bundle": "rolldown ./extension.js -d dist -f cjs -e vscode -s",
    "watch": "npm run bundle -- -w",
    "release": "vsce publish",
    "vscode:prepublish": "npm run bundle"
  },
  "dependencies": {
    "markdown-it-ruby": "1.1.2"
  },
  "devDependencies": {
    "@vscode/vsce": "3.6.2",
    "rolldown": "^1.0.0-beta.43"
  }
}
